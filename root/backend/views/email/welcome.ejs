                  <table
                    role="presentation"
                    border="0"
                    cellpadding="0"
                    cellspacing="0"
                  >
                    <tbody>
                      <tr>
                        <td>
                          <!-- prettier-ignore -->
                          <a href="<%=url %>" target="_blank">Go to Campy</a>
